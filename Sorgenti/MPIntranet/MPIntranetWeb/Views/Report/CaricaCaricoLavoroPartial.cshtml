@using MPIntranet.Models.Report
@model List<CaricoRepartoModel>

@if (Model != null && Model.Count() == 0)
{
    <text><b>Nessun risultato trovato</b></text>
}
else
{
    <table id="grigliaRisultati" class="table table-condensed table-hover">
        <thead style="background-color:aliceblue">
            <tr>
                <th width="10%">Azienda</th>
                <th width="10%">Destipolancio</th>
                <th width="10%">Codice Tipo</th>
                <th width="10%">Ragione Soc</th>
                <th width="5%">Nummovfase</th>
                <th width="5%">NomeCommessa</th>
                <th width="5%">Segnalatore</th>
                <th width="10%">Codtipomovfase</th>
                <th width="10%">Modello_lancio</th>
                <th width="5%">Modello_Wip</th>
                <th width="5%">Elenco Fasi</th>
                <th width="5%">Datamovfase</th>
                <th width="5%">Qta</th>
                <th width="5%">Qtadater</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (CaricoRepartoModel td in Model)
            {
            <tr>
                <td width="10%">
                    @td.Azienda
                </td>
                <td width="10%">
                    @td.Destipolancio
                </td>
                <td width="10%">
                    @td.Codicetipoo
                </td>
                <td width="10%">
                    @td.Ragionesoc
                </td>
                <td width="5%">
                    @td.Nummovfase
                </td>
                <td width="5%">
                    @td.Nomecommessa
                </td>
                <td width="5%">
                    @td.Segnalatore
                </td>
                <td width="10%">
                    @td.Codtipomovfase
                </td>
                <td width="10%">
                    @td.Modello_Lancio
                </td>
                <td width="5%">
                    @td.Modello_Wip
                </td>
                <td width="5%">
                    @td.Elencofasi
                </td>
                <td width="5%">
                    @td.Datamovfase
                </td>
                <td width="5%">
                    @td.Qta
                </td>
                <td width="5%">
                    @td.Qtadater
                </td>
                <td></td>
            </tr>
                }

        </tbody>
        </table>
    }